{
  "name": "react-mvx",
  "version": "1.0.0-a05",
  "repository": {
    "type": "git",
    "url": "https://github.com/gaperton/React-MVx.git"
  },
  "description": "React MV* framework",
  "main": "./dist/index.js",
  "module": "./lib/src/index.js",
  "types": "./lib/src/index.d.ts",
  "files": [
    "dist",
    "lib",
    "tags.jsx",
    "tags.js"
  ],
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "pull": "git submodule update --remote && cp ./submodules/NestedLink/tags.* . && cp ./submodules/NestedLink/src/* ./src/valuelink",
    "compile": "./node_modules/.bin/tsc && ./node_modules/.bin/webpack -p",
    "deploy": "cp -R ./dist ./examples/*/node_modules/react-mvx",
    "build": "npm run pull && npm run compile",
    "docs" : "rm -rf ./docs/* && cd ./documentation && npm run build && cd .. && mv ./documentation/build/* ./docs"
  },
  "keywords": [
    "react",
    "mvc",
    "model",
    "state",
    "mvvm"
  ],
  "author": "Vlad Balin",
  "license": "MIT",
  "devDependencies": {
    "@types/prop-types": "*",
    "@types/react": "*",
    "prop-types": "*",
    "react-dom": "*",
    "source-map-loader": "*",
    "ts-loader": "*",
    "typescript": "*",
    "webpack": "*",
    "react": "*",
    "type-r": "^1.0.0-rc6"
  },
  "peerDependencies": {
    "prop-types": "*",
    "react": "*",
    "type-r": "^1.0.0-rc6"
  }
}
